import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int arr[] = new int[n];
        for (int i = 0; i < n; i++) {
            arr[i] = scanner.nextInt();
            if (arr[i] % 3 == 0) {
                System.out.println("Second");
            } else {
                System.out.println("First");
            }
        }
        scanner.close();
    }
}
